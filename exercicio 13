#include <stdio.h>
int main() {
int C;
float F;
printf("Digite a temperatura em Celsius: ");
scanf("%d", &C);
F = (9 * C + 160) / 5.0;
printf("Temperatura em Fahrenheit: %.2f\n", F);
return 0;
}
Exerc√≠cio 14
#include <stdio.h>
int main() {
int A, B, AUX
printf("Digite o valor de A: ");
scanf("%d", &A);
printf("Digite o valor de B: ");
scanf("%d", &B);
AUX = A;
A = B;
B = AUX;
printf("Novo valor de A: %d\n", A);
printf("Novo valor de B: %d\n", B);
return 0;
}
